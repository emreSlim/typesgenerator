{
  "name": "typesgenerator",
  "version": "1.3.0",
  "description": "This package helps to create typescript types/interfaces from object or json",
  "main": "dist/cjs/lib/index.js",
  "types": "dist/cjs/lib/index.d.ts",
  "bin": {
    "tg": "dist/cjs/bin/index.js"
  },
  "type": "commonjs",
  "scripts": {
    "test": "npm run build &&  node test.js",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "clean": "rm -r dist || true",
    "testInstallDev": "npm uninstall -g typesgenerator && npm install -g ./",
    "testCLI": "tg -s person.json -n person",
    "build:esm": "tsc -p ./tsconfig.esm.json ",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "npm run build"
  },
  "keywords": [
    "create",
    "interface",
    "create",
    "types",
    "generate",
    "generator",
    "typscript",
    "types",
    "interfaces",
    "light",
    "tool"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/lib/index.js"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/lib/index.js"
      }
    }
  },
  "files": [
    "dist/**/*"
  ],
  "author": "emreSlim",
  "license": "ISC",
  "devDependencies": {
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "typesgenerator": "file:./",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
